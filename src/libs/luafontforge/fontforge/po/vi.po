msgstr "Hình tượng « %s » chứa tham chiếu đến %s trong %s\n"

msgid "Glyph “%s” refers to %s with a different transformation matrix\n"
msgstr "Hình tượng « %s » tham chiếu đến %s bằng ma trận chuyển dạng khác\n"

msgid "A match was found after unlinking references in glyph “%s”\n"
msgstr "Đã khớp sau khi bỏ liên kết tham chiếu trong hình tượng « %s »\n"

msgid "Glyph “%s” has advance width %d in %s but %d in %s\n"
msgstr "Hình tượng « %s » có độ rộng tiến tới %d trong %s còn là %d trong %s\n"

msgid "Glyph “%s” has vertical advance width %d in %s but %d in %s\n"
msgstr "Hình tượng « %s » có độ rộng tiến tới dọc %d trong %s còn là %d trong %s\n"

msgid "Glyph “%s” has a different number of layers\n"
msgstr "Hình tượng « %s » có số lớp khác\n"

msgid "Glyph “%s” has a different fill in layer %d\n"
msgstr "Hình tượng « %s » có tô đầy khác trong lớp %d\n"

msgid "Glyph “%s” has a different stroke in layer %d\n"
msgstr "Hình tượng « %s » có nét khác trong lớp %d\n"

msgid ""
"Glyph “%s” does not have splines which match exactly, but they are close\n"
msgstr "Hình tượng « %s » không có chốt trục khớp chính xác, nhưng là tương tự\n"

msgid "Different number of contours in glyph “%s”\n"
msgstr "Hình tượng « %s » có số đường mức khác\n"

msgid "Open/Closed contour mismatch in glyph “%s”\n"
msgstr "Hình tượng « %s » có đường mức mở/đóng không khớp\n"

msgid "Spline mismatch in glyph “%s”\n"
msgstr "Hình tượng « %s » có chốt trục không khớp\n"

msgid "Hint masks differ in glyph “%s” at (%g,%g)\n"
msgstr "Hình tượng « %s » có mặt nạ mẹo khác tại (%g,%g)\n"

msgid "Hints differ in glyph “%s”\n"
msgstr "Hình tượng « %s » có mẹo khác\n"

