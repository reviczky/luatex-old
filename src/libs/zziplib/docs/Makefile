                 $(srcdir)/make-doc.pl ../zzip/*.c
	- /usr/bin/perl5 $(srcdir)/make-doc.pl ../zzip/*.c \
          "suffix=0" "package=$(PACKAGE)" "version=$(VERSION)" \
          "mainheader=zzip/zzip.h" "exported=<zzip/zzip.h> // or <zziplib.h>" \
	  "docbookfile=$@" 
	- test -s $@
